CELERYD_NODES="worker1"
CELERY_BIN="<%= @superset['base_dir'] %>/venv/bin/celery"
CELERY_APP="superset.tasks.celery_app"
CELERYD_CHDIR="<%= @superset['base_dir'] %>"
CELERYD_OPTS="--pool=prefork -O fair --concurrency=<%= @concurrency %>"
CELERYD_LOG_LEVEL="<%= @log_level %>"
CELERYD_LOG_FILE="/var/log/celery-<%= @superset['name'] %>/%n%I.log"
CELERYD_PID_FILE="/var/run/celery-<%= @superset['name'] %>/%n.pid"
CELERYD_USER="<%= @owner %>"
CELERYD_GROUP="<%= @group %>"
CELERY_CREATE_DIRS=1
